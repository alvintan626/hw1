// puzzle 3

function repeat(direction,total) {
  var count = 0;
  while (count < total) {
    direction();
    count = count + 1;
  }
}

repeat(down,2)
var mycolor = getColor()    // obtains color that rudy is on
repeat(down,3)
repeat(right,2)
setColor(mycolor)         // matches the color obtained from mycolor memory and unlocks gate next to rudy
repeat(right,2)
up()
