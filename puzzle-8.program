//puzzle 8

function repeat(direction,total) {
  var count = 0;
  while (count < total) {
    direction();
    count = count + 1;
  }
}

// pattern:
// down up down up down

repeat(down,8)
repeat(right,2)
repeat(up,7)
repeat(right,2)
repeat(down,7)
repeat(right,2)
repeat(up,7)
repeat(right,2)
repeat(down,7)
right()
